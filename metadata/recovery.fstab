#	Android	fstab
#<src>	<mnt_point>	<type>
#	The	filesystem
#	specify	MF_CHECK,
		
#	Internal	Storage
/boot	emmc	/dev/block/mmcblk0p40
/system	ext4	/dev/block/mmcblk0p43
/cache	ext4	/dev/block/mmcblk0p42
/data	ext4	/dev/block/mmcblk0p44
		
#	because	/proc/emmc
/misc	emmc	/dev/block/mmcblk0p24
		
#	External	Storage
/external_sd	vfat	/dev/block/mmcblk1p1
/usb-otg	vfat	/dev/block/sda1
/sdcard	vfat	/dev/block/mmcblk1
